# Instagram Credentials
INSTAGRAM_USERNAME=your_username
INSTAGRAM_PASSWORD=your_password
ALLOW_FRESH_LOGIN=true

# Bot Configuration
BOT_PREFIX=.
ADMIN_USERS=user_id_1,user_id_2
ENABLE_LOGGING=true
LOG_LEVEL=INFO
HEARTBEAT_INTERVAL=300000
MESSAGE_TIMEOUT=30000

# Realtime Configuration
REALTIME_AUTO_RECONNECT=true
REALTIME_ENABLE_TRACE=false
USE_IRIS_DATA=true
RECONNECT_DELAY=5000

# Push Configuration
PUSH_AUTO_RECONNECT=true
PUSH_ENABLE_TRACE=false

# Module Configuration
AUTO_LOAD_MODULES=true
ENABLED_MODULES=core-commands,auto-responder,media-handler
